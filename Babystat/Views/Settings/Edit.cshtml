@model Babystat.Models.SettingsViewModels.SettingsEditVm
@{
    ViewData["Title"] = "Settings";
}

<h3>Settings</h3>

<form method="post">
    <div class="form-group">
        <label asp-for="ActiveBabyId" class="control-label">Active Baby</label>
